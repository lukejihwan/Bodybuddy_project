<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="GpsData">


	<!-- 하루를 기준으로 데이터 가져오기! -->
	<select id="selectForDay" parameterType="String" resultType="GpsData">
		select * from gpsdata
		where regdate = #{regdate}
	</select>
	
	

	<!-- Member 나중에 추가 예정 -->
	<insert id="insert" parameterType="GpsData">
		insert into gpsdata(lati, longi, regdate)
		values(#{lati}, #{longi}, #{regdate})
	</insert>


	<delete id="delete" parameterType="int">
		delete from gpsdata
		where exr_record_idx=#{exrRecord.exr_record_idx}
	</delete>
	

</mapper>